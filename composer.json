{
    "name": "ttree/flowplatformsh",
    "description": "Flow framework utility package to parse Platform.sh variables",
    "type": "neos-package",
    "license": "MIT",
    "require": {
        "neos/flow": "^4.0",
        "m1/env": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Ttree\\FlowPlatformSh\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Ttree.FlowPlatformSh"
        },
        "class": "Ttree\\FlowPlatformSh\\Composer\\InstallerScripts",
        "branch-alias": {
          "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
      "post-update-cmd":  "Ttree\\FlowPlatformSh\\Composer\\InstallerScripts::postUpdateAndInstall",
      "post-install-cmd": "Ttree\\FlowPlatformSh\\Composer\\InstallerScripts::postUpdateAndInstall"
    }
}
